config_version=5

[application]

config/name="Game Jam 80 Team"
run/main_scene="uid://bxroi0e1f7wuh"
config/features=PackedStringArray("4.4", "Forward Plus")
config/icon="res://icon.svg"

[audio]

buses/default_bus_layout="res://scenes/audio/default_bus_layout.tres"

[autoload]

SaveLoad="*res://src/SaveLoad.gd"
Game="*res://src/game.gd"
SignalBus="*res://src/signal_bus.gd"
SettingsDataContainer="*res://src/settings_data_container.gd"
SignalBusSettings="*res://src/signal_bus_settings.gd"
SaveManager="*res://src/save_manager.gd"
MusicPlayer="*res://src/music_player.gd"

[display]

window/size/viewport_width=1920
window/size/viewport_height=1080
window/size/resizable=false
window/stretch/mode="canvas_items"

[editor]

version_control/plugin_name="GitPlugin"
version_control/autoload_on_startup=true

[editor_plugins]

enabled=PackedStringArray("res://addons/csv-data-importer/plugin.cfg")

[global_group]

Player=""

[importer_defaults]

com.timothyqiu.godot-csv-importer={
"delimiter": 0,
"detect_numbers": true,
"force_float": true,
"headers": true
}

